<script>
	$(document).ready(function(){
	    // $(".fIzq").click(function(){
	    // 	$(".fbn:nth-child(5)").animate({'margin'});
	    // });

		$('#demo3').boutique({
			front_img_width:	158,
			front_img_height:	233,
			autoplay:			false,
			autoplay_interval:	4000,
			front_topmargin:	-15,
			behind_topmargin:	0,
			back_topmargin:		0,
			behind_size:		1,
			back_size:			1,
			behind_distance:	160,
			behind_opacity:		1,
			back_opacity:		1,
			move_more_directly: true,
			text_opacity:		0,
			frames:				5,
			starter:			3,
			speed:				500,
			hovergrowth:		0.05,
			freescroll:			true,
			text_front_only:	true,
			keyboard:			false
		});
	});
</script>
